sshpass -f /home/pass/spass ssh root@192.168.1.1 -C "ls -rt /home/oracle/| tail -n2" > /home/oracle_logs/loginfo
#cat /tmp/loginfo | awk -F' ' '{print $NF}' >> /tmp/newinfo
tt=$(cat  /home/oracle_logs/loginfo | head -n 1)
yy=$(cat  /home/oracle_logs/loginfo | tail -n1)
uu=$(date +"%F %T")
mkdir /home/oracle_logs/"$uu"
scp user@ip:/home/oracle/$tt  /home/superuser/oracle_logs/"$uu"/
scp user@ip:/home/oracle/$yy /home/superuser/oracle_logs/"$uu"/
