ip


[win:vars]
ansible_user=admin_user
ansible_password=admin_pass
ansible_connection=winrm
#ansible_host=ip
ansible_winrm_transport=ntlm
ansible_winrm_scheme=http
ansible_winrm_port=5985
ansible_winrm_server_cert_validation=ignore



ansible-doc -l | grep win_  #get windows module


anisble -i hosts win -m win_ping## check windows ping 
ansible -i hosts ip -m win_command -a "sc /?"  ## sc quesry help cmds


ansible -i hosts ip -m win_command -a 'wmic product get name, version'# get installed app list


##caling reinstall for package adobe
ansible -i hosts ip -m win_command -a 'wmic product where name="Adobe Acrobat Reader DC" call reinstall ' 

